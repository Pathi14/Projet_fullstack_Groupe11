{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "jsx": "react",
      "module": "esnext",
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": ["src"]
  }
  